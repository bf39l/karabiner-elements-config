{
    "description": "05 - Meh - Apps",
    "manipulators": [
        {
            "description": "iTerm",
            "from": {
                "key_code": "f",
                "modifiers": { "mandatory": ["left_shift", "left_control", "left_option"] }
            },
            "to": [{ "software_function": { "open_application": { "file_path": "/Applications/iTerm.app" } } }],
            "type": "basic"
        },
        {
            "description": "Launchpad",
            "from": {
                "key_code": "a",
                "modifiers": { "mandatory": ["left_shift", "left_control", "left_option"] }
            },
            "to": [{ "key_code": "launchpad" }],
            "type": "basic"
        },
        {
            "description": "Google Chrome",
            "from": {
                "key_code": "w",
                "modifiers": { "mandatory": ["left_shift", "left_control", "left_option"] }
            },
            "to": [{ "software_function": { "open_application": { "file_path": "/Applications/Google Chrome.app" } } }],
            "type": "basic"
        }
    ]
}